<div class="[ page-header ] [ animate__animated animate__fadeIn ]">
  <div class="[ wrapper ] [ flow flow-space-500 ]">
    <h1 class="[ page-header__heading ] [ headline ]">
      {{ pageHeaderTitle }}
    </h1>
    {% if pageHeaderSummary %}
    <div class="[ page-header__summary ] [ measure-short ]">
      {{ pageHeaderSummary | safe }}
    </div>
    {% endif %}
  </div>
</div>
