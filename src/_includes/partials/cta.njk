{% set ctaPrefix = cta %}

{% if ctaContent %} 
  {% set ctaPrefix = ctaContent %}
{% endif %}

<article class="[ cta ] [ panel ]">
  <div class="wrapper">
    <div class="[ cta__inner ] [ flow ]">
      <h2 class="[ cta__heading ] [ headline ]">{{ ctaPrefix.title }}</h2>
      <p class="[ cta__summary ] [ measure-short ] [ font-header-light ]">{{ ctaPrefix.summary | safe }}</p>
    </div>
  </div>
</article>
