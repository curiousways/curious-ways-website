{% extends "layouts/base.njk" %} 
{% set pageCriticalStyles = ['css/home.css'] %}
{% set bodyClass = 'page-home bg bg--home home-video--load' %}
{% set pageID = 'page-home' %}
{% set pageScript = '/js/home-video.js' %}

{% block content %}

<div id="home-video-wrapper"></div>

<div class="wrapper">
	<article class="home">
		<div class="[ home__content ] [ flow ] [ has-special-links line-break--xl ]">
			<h1 class="home__title font-header-light">{{ home.title | safe  }}</h1>
			<h2 class="home__subtitle font-header-light">{{ home.subtitle  | safe }}</h2>
			<p class="home__cta font-header-light">{{ home.cta  | safe }}</p>
		</div>
	</article>
</div>

{% endblock %}